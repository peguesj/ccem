{
  "servers": {
    "yjos": {
      "name": "yjos",
      "url": "https://yjos.lgtm.build",
      "auth": {
        "type": "api-key",
        "tokenEnv": "YJOS_API_KEY"
      },
      "retry": {
        "maxAttempts": 3,
        "backoff": "exponential",
        "initialDelayMs": 1000
      },
      "timeoutMs": 5000,
      "enabled": true
    }
  }
}
