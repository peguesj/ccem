{
  "name": "@ccem/core",
  "version": "1.0.0",
  "description": "Claude Code Environment Manager - TUI-based configuration management system for Claude Code v2.0.10+",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ccem": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src tests --ext .ts,.tsx",
    "lint:fix": "eslint src tests --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"tests/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\" \"tests/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "docs": "typedoc --out docs src",
    "prepublishOnly": "npm run lint && npm run test:coverage && npm run build",
    "ui:dev": "cd ui && npm run dev",
    "ui:build": "cd ui && npm run build",
    "ui:preview": "cd ui && npm run preview",
    "ui:test": "cd ui && npm run test",
    "ui:lint": "cd ui && npm run lint",
    "ui:typecheck": "cd ui && npm run typecheck"
  },
  "keywords": [
    "claude",
    "claude-code",
    "cli",
    "tui",
    "configuration",
    "management",
    "developer-tools"
  ],
  "author": "Jeremiah Pegues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/peguesj/ccem.git"
  },
  "bugs": {
    "url": "https://github.com/peguesj/ccem/issues"
  },
  "homepage": "https://github.com/peguesj/ccem#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@types/react": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "babel-jest": "^30.2.0",
    "eslint": "^8.56.0",
    "happy-dom": "^20.0.11",
    "husky": "^8.0.3",
    "ink-testing-library": "^3.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "playwright": "^1.57.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.7",
    "typescript": "^5.3.3",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/ws": "^8.18.1",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "ink": "^4.4.1",
    "react": "^18.2.0",
    "ws": "^8.18.3",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
